@model OOD.Models.ProfileViewModel;
@*@{
    ViewData["Title"] = "DashBoard";
    }*@
<!DOCTYPE html>
<html>
<head>
    <title>Dashboard</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!--favicon-->
    <link rel="shortcut icon" href="../images/fav.png" type="../image/x-icon">
    <!-- ! css Begin-->
    <link rel="stylesheet" type="text/css" href="../css/dashboard-app/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="../css/dashboard-app/screen.css">
    <!-- ! css ends -->
    <!-- ! Custom fonts Begin -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">
    <!-- ! Custom fonts end -->
    <!-- FontAwsome -->
    <script src="https://kit.fontawesome.com/5336317b7a.js" crossorigin="anonymous"></script>

</head>
<body>
    <div class="dashboardScreenWrapper d-flex">
        <div class="mainMenuWrapper mainequalheight">
            <div class="menuIcon">
                <a href="javascript:void(0)"><i class="fas fa-bars"></i></a>
            </div>
            <div class="logo">
                <a href="/Home/Index" class="fullLogo"><img src="../images/logo-2.png" title="Unblock Insta"></a>
                <a href="/Home/Index" class="logoIcon"><img src="../images/logo-icon.png" title="Unblock Insta"></a>

            </div>
            <div class="menulist">
                <ul>
                    <li>
                        <a href='/Home/DashBoard' class="active"><i class="fas fa-tachometer-alt"></i> Dashboard</a>
                    </li>
                    <li>
                        <a href='/Home/Profile'><i class="fas fa-border-all"></i> My Profile</a>
                    </li>
                    <li>
                        <a href='/Home/Form'><i class="far fa-file"></i> New Form</a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="contentPart mainequalheight">
            <div class="header d-flex justify-content-end align-items-center flex-wrap flex-md-nowrap">
                <div class="calenderNotification">
                    <a href="#">
                        <i class="far fa-bell"></i>
                        <span class="notificationBtn">10</span>
                    </a>
                </div>
                <div class="userProfileicon">
                    <div class="userIcon coverbg" style="background-image: url('../images/user-img.png')"></div>
                    <div class="userstatus"></div>
                </div>
            </div>
            <div class="title">
                <h1>Dashboard</h1>
                <h2>Dashboard & statistics</h2>
            </div>
            <div class="appContentMain">
                <div class="row">
                    <div class="col-md-6 mb-3 mb-md-0  mb-lg-3 mb-xl-0">
                        <div class="notecard">
                            <div class="editPrifle">
                                <a href="/Home/Profile">Edit</a>
                            </div>
                            <h5 class="text-blue">User Profile <span></span></h5>
                            <div class="userDetails">
                                <p class="mb-3"><span class="mr-2"><strong>First Name</strong></span><span>@ViewBag.FirstName</span></p>
                                @*@Html.LabelFor(model => model.FirstName)*@
                                <p class="mb-3"><span class="mr-2"><strong>Last Name</strong></span><span>@ViewBag.LastName</span></p>
                                <p class="mb-3"><span class="mr-2"><strong>Email</strong></span><span>@ViewBag.EmailAddress</span></p>
                                <p class="mb-3"><span class="mr-2"><strong>Contact Number</strong></span><span>-</span></p>
                                <p class="mb-3"><span class="mr-2"><strong>Date of Birth</strong></span><span>-</span></p>
                                <p class="mb-3"><span class="mr-2"><strong>State of living</strong></span><span>@ViewBag.State</span></p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-sm-6 mb-3 mb-md-0  mb-lg-3 mb-xl-0">
                        <div class="class-device">
                            <h5 class="text-blue">Device Management<span></span></h5>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="notecard">
                                        <div class="editPrifle">
                                            <a href="#">Edit</a>
                                        </div>
                                        <div class="userDetails">
                                            <p>
                                                <i class="fa fa-mobile" style="font-size:38px;"> </i> <span style="font-size:16px;">Iphone2211</span>
                                            </p>
                                            <p>Last Synced:03/22/2022</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 mb-3 mb-md-0  mb-lg-3 mb-xl-0">
                                    <div class="notecard">
                                        <div class="editPrifle">
                                            <a href="#">Edit</a>
                                        </div>
                                        <div class="userDetails">
                                            <p>
                                                <i class="fa fa-mobile" style="font-size:38px;"> </i> <span style="font-size:16px;">Fitbit2010</span>
                                            </p>
                                            <p>Last Synced:03/22/2022</p>
                                        </div>
                                    </div>
                                </div>
                                <h5></h5>
                                <div class="col-sm-12 align-self-end text-md-right" style="margin-top:70px; ">
                                    <button type="submit" class="buttonDark">Add Wearables</button>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>


                <div class="referralLinkSection ">
                    <div class="row">
                        <div class="col-6">
                            <h5 class="text-blue">Previous records <span></span></h5>
                        </div>
                        <div class="col-6">
                            <div class="editPrifle">
                                <a href="#">View Details</a>
                            </div>
                        </div>
                        <div class="col-12" style="margin-top:30px">
                            <table class="table table-striped tdcenterValue large-table-responsive">
                                <thead>
                                    <tr>
                                        <th class="text-uppercase"><i></i>Date</th>
                                        <th class="text-uppercase"><i></i>Had any covid symptoms</th>
                                        <th class="text-uppercase"><i></i>Temperature (°F)</th>
                                        <th class="text-uppercase"><i></i>Regular medicine</th>
                                        <th class="text-uppercase"><i></i>Had met anyone</th>
                                        <th class="text-uppercase"><i></i>Had met person has Covid</th>
                                        <th class="text-uppercase"><i></i>Had travel history</th>
                                        <th class="text-uppercase"><i></i>Risk Result</th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>1-March-2022</td>
                                        <td>No</td>
                                        <td>96</td>
                                        <td>No</td>
                                        <td>No</td>
                                        <td>No</td>
                                        <td>No</td>
                                        <td><span class="text-green"><i class="far fa-check-circle"></i></span></td>
                                    </tr>
                                    <tr>
                                        <td>11-March-2022</td>
                                        <td>Yes</td>
                                        <td>110</td>
                                        <td>Yes</td>
                                        <td>Yes</td>
                                        <td>Yes</td>
                                        <td>Yes</td>
                                        <td><span class="text-danger"><i class="far fa-times-circle"></i></span></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <!-- ! Script Begin -->
    <script src="../js/function.js"></script>
    <script src="../js/global.js"></script>
    <!-- ! Script ends -->

    <script type="text/javascript">

    </script>
</body>
</html>